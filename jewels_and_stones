class Solution {
    public int numJewelsInStones(String J, String S) {
        HashMap<Character,Integer> map = new HashMap<>();
        int count=0;
        for(int i=0;i<J.length();i++)
            map.put(J.charAt(i),1);
        for(int i=0;i<S.length();i++)
        {
            
            if(map.containsKey(S.charAt(i)))
                count++;
            }
        return count;
    }
}
